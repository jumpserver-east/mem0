{
    "mem0": {
        "llm": {
            "provider": "openai",
            "config": {
                "model": "env:OPENAI_API_MODEL",
                "temperature": 0.1,
                "max_tokens": 15000,
                "api_key": "env:OPENAI_API_KEY",
                "openai_base_url": "env:OPENAI_BASE_URL"
            }
        },
        "embedder": {
            "provider": "openai",
            "config": {
                "model": "env:OPENAI_EMBEDDING_MODEL",
                "api_key": "env:OPENAI_API_KEY",
                "embedding_dims": 3072,
                "openai_base_url": "env:OPENAI_BASE_URL"
            }
        }
    }
} 